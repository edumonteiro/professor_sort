<div class='well col-md-offset-3' style='width: 30%'>
  <%= form_for @professor,url: professor_path do |f| %>
    <div class='form-group'>
      <%= f.label :name %><br>
      <%= f.text_field :name, placeholder:'name', class: 'form-control' %>
    </div>
    <div class='form-group'>
      <%= f.label :email %><br>
      <%= f.text_field :email, placeholder:'email', class: 'form-control' %>
    </div>
    <div class='form-group'>
      <%= f.label :password %><br>
      <%= f.password_field :password, placeholder: 'password', class: 'form-control' %>
    </div>
    <div class='form-group'>
      <%= f.label :password_confirmatioin %><br>
      <%= f.password_field :password_confirmatioin, placeholder: 'password', class: 'form-control' %>
    </div>
    <div class='form-group'>
      <%= f.label :sign_up_code %><br>
      <%= f.text_field :kind, class: 'form-control' %>
    </div>
    <div class='actions'>
      <%= f.submit 'submit',class: 'btn btn-default' %>
    </div>
  <% end %>
</div>  